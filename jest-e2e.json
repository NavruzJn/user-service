{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "coverageDirectory": "./coverage",
  "testRegex": ".e2e.test.ts$",
  "globalSetup": "<rootDir>/tests/jestGlobalSetup.ts",
  "moduleNameMapper": {
    "@src/(.*)$": "<rootDir>/src/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
