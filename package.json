{
  "name": "user-service",
  "version": "1.0.0",
  "description": "HTTP User Micro Service",
  "main": "index.js",
  "scripts": {
    "test": "echo \\\"Error: no tests specified\\\" && exit 1",
    "test:e2e": "jest -i --forceExit --config jest-e2e.json",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "yarn migration:run && yarn seed:run && nest start --watch",
    "typeorm": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./node_modules/.bin/typeorm",
    "migration:generate": "yarn typeorm -- migration:generate src/persistence/migrations/initialize-tables -d src/persistence/configs/datasource-config.ts",
    "migration:run": "yarn typeorm -- migration:run  -d src/persistence/configs/datasource-config.ts",
    "migration:revert": "yarn typeorm -- migration:revert  -d src/persistence/configs/datasource-config.ts",
    "migration:all": "yarn migration:revert && yarn migration:run",
    "seed:run": "ts-node -r tsconfig-paths/register --project ./tsconfig.json src/persistence/migrations/seeds/create-user.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/swagger": "^11.1.6",
    "@nestjs/testing": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "env-var": "^7.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.3.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.22",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@nestjs/cli": "^11.0.7",
    "@types/express": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/request": "^2.48.12",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
